
<body>
    <h1> Edit <%= @band.name %> </h1>

    <form action="<%= band_url %>" method = "POST">
    <input type="hidden" name = "_method" value = "PATCH">
    <input type="hidden" name = "authenticity_token" value = "<%= form_authenticity_token%>">
    <label for="name">Edit Band Name:</label>
    <input type="text" id = "name" value = "<%=@band.name%>" name ="band[name">
    <br/><br/>
    <input type="submit" value = "Edit Band">

    </form>
</body>